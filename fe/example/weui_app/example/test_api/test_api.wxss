.test-section {
  padding: 20rpx;
  background: #f5f5f5;
  margin: 20rpx;
  border-radius: 10rpx;
}

.test-buttons {
  display: flex;
  flex-wrap: wrap;
  gap: 10rpx;
  margin-bottom: 20rpx;
}

.test-buttons button {
  flex: 1;
  min-width: 150rpx;
  font-size: 24rpx;
  padding: 10rpx;
}

.test-results {
  background: white;
  padding: 20rpx;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
}

.result-title {
  font-weight: bold;
  margin-bottom: 10rpx;
}

.result-item {
  border-bottom: 1rpx solid #eee;
  padding: 10rpx 0;
  font-size: 24rpx;
}

.result-item text {
  display: block;
  margin: 2rpx 0;
}

.time { color: #666; font-size: 20rpx; }
.api { color: #1890ff; font-weight: bold; }
.status.success { color: #52c41a; }
.status.error { color: #ff4d4f; }
.data { color: #333; word-break: break-all; }

.live-data {
  background: white;
  padding: 20rpx;
  border-radius: 10rpx;
}

/*******************按钮**/
/* 导航按钮样式 */
.nav-section {
  margin: 30rpx 0;
  text-align: center;
}

.nav-btn {
  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
  border: none;
  border-radius: 50rpx;
  padding: 25rpx 40rpx;
  color: white;
  font-size: 30rpx;
  font-weight: bold;
  box-shadow: 0 8rpx 25rpx rgba(102, 126, 234, 0.3);
  transition: all 0.3s ease;
  display: inline-flex;
  align-items: center;
  justify-content: center;
}

.nav-btn:active {
  transform: translateY(2rpx);
  box-shadow: 0 4rpx 15rpx rgba(102, 126, 234, 0.4);
}

.nav-btn-text {
  color: white;
  font-weight: bold;
}


